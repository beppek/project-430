{
  "name": "project",
  "version": "1.0.0",
  "description": "Application providing photo challenges weekly",
  "main": "server.js",
  "scripts": {
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-css": "node-sass --include-path scss source/sass/style.scss client/assets/css/style.css",
    "start": "npm run build && npm run serve",
    "test": "mocha",
    "test-coverage": "mocha test --require blanket --reporter html-cov > coverage.html",
    "build": "browserify source/js/app.js -o client/assets/js/bundle.js ",
    "serve": "nodemon --watch server.js server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1dv430/bk222bh-project.git"
  },
  "keywords": [
    "NodeJS",
    "AngularJS",
    "Express",
    "Photography",
    "Challenge"
  ],
  "author": "beppek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1dv430/bk222bh-project/issues"
  },
  "homepage": "https://github.com/1dv430/bk222bh-project#readme",
  "dependencies": {
    "angular": "^1.5.3",
    "angular-animate": "^1.5.5",
    "angular-route": "^1.5.3",
    "angular-ui-router": "^0.2.18",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "connect-redis": "^3.0.2",
    "crypto": "0.0.3",
    "csurf": "^1.8.3",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "jquery": "^2.2.3",
    "jwt-simple": "^0.5.0",
    "moment": "^2.13.0",
    "mongoose": "^4.4.11",
    "node-sass": "^3.5.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "request": "^2.72.0",
    "satellizer": "^0.14.0"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "karma": "^0.13.22",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "client/src/js/app.js"
      ],
      "data-cover-never": "node_modules"
    }
  }
}
